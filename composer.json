{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.*",
        "barryvdh/laravel-ide-helper": "^2.3",
        "barryvdh/laravel-debugbar": "^2.3",
        "acacha/admin-lte-template-laravel": "^4.0",
        "acacha/periods": "dev-master",
        "acacha/names": "dev-master",
        "acacha/user": "~0.2",
        "acacha/stateful-eloquent": "dev-master",
        "laravel/passport": "^2.0",
        "scool/foundation": "0.2.1",
        "acacha/l5-repository": "dev-master",
        "predis/predis": "^1.1",
        "spatie/laravel-menu": "^2.1.2",
        "spatie/laravel-permission": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/tinker": "^1.0",
	"laravel/socialite": "^3.0",
        "laravel/browser-kit-testing": "^1.0",
        "acacha/laravel-social": "^0.1.4",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "maatwebsite/excel": "^2.1",
        "intervention/image": "^2.3",
        "laravelcollective/html": "^5.3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "phpspec/phpspec": "^3.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
